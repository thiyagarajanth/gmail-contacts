<table>
	<tbody>
		<h2>Please enter your Gmail</h2>
		<%= form_tag '/show_contacts' %>
			<tr>
				<td><%= label_tag :Email%></td>
				<td><%= text_field_tag :email%></td>
			</tr>
			<tr>
				<td><%= label_tag :password%></td>
				<td><%= password_field_tag :password%></td>
			</tr>
			<tr>
				<td colspan="2"><%= submit_tag :login %></td>
			</tr>
	</tbody>
</table>